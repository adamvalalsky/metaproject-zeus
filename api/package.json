{
  "name": "resource-manager-backend",
  "version": "0.0.1",
  "author": "Adam Valalsk√Ω",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"./**/*.{ts,tsx,js}\" && tsc --noEmit",
    "lint:fix": "eslint --fix \"./**/*.{ts,tsx,js}\"",
    "lint:ci": "eslint \"./**/*.{ts,tsx,js}\"  --format junit --output-file test/_output/eslint.xml && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/config/jest-e2e.json",
    "test:e2e:ci": "JEST_JUNIT_OUTPUT_FILE=./test/_output/jest-e2e.xml jest --config ./test/config/jest-e2e.json --ci --reporters=default --reporters=jest-junit",
    "test:unit": "jest --config ./test/config/jest-unit.json",
    "test:unit:ci": "JEST_JUNIT_OUTPUT_FILE=./test/_output/jest-unit.xml jest --config ./test/config/jest-unit.json --ci --reporters=default --reporters=jest-junit",
    "prepare": "cd .. && husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-session": "^1.18.0",
    "joi": "^17.11.0",
    "passport": "^0.7.0",
    "passport-openidconnect": "^0.1.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "resource-manager-database": "../shared/database",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-openidconnect": "^0.1.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
